{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"allowCompoundWords": false,
	"dictionaries": [
		"project-words"
	],
	"dictionaryDefinitions": [
		{
			"addWords": true,
			"name": "project-words",
			"path": "./words-project.txt"
		}
	],
	"ignorePaths": [
		"**/*.svg",
		"./.github.disabled/**",
		"./LICENSE",
		"./i18n/**",
		"./node_modules",
		"./tests/scripts/*",
		"./words-*.txt"
	],
	"ignoreWords": [],
	"language": "en,en-ca,en-gb",
	"version": "0.2"
}
